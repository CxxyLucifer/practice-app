    "rc-dialog": "~6.5.7",
    "rc-form": "^2.1.4",
    "rc-input-number": "3.4.13",
    "rc-swipeout": "^1.2.4",
    "react": "16.0.0-alpha.12",
    "react-dom": "16.0.0-alpha.12",
    "react-mixin": "2.0.2",
